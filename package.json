{
  "name": "express-api-template",
  "version": "1.0.0",
  "description": "My personal express api template so i don't have to start from scratch every time...",
  "main": "index.js",
  "scripts": {
    "dev": "tsc && node --env-file ./src/config/.env.local dist/config/server.js",
    "format": "prettier --write 'src/**/*.{js,ts,tsx,json,md}'",
    "lint": "eslint --ext .js,.ts,.tsx src",
    "build": "tsc",
    "start": "node --env-file ./src/config/.env.local dist/config/server.js"
  },
  "author": "Florian Lepage",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.4.2",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "prisma": "^5.4.2"
  }
}
