{
  "name": "express-api-template",
  "version": "1.0.0",
  "description": "My personal express api template so i don't have to start from scratch every time...",
  "main": "index.js",
  "scripts": {
    "dev": "tsc && node --env-file ./src/config/.env.local dist/config/server.js",
    "format": "prettier --write 'src/**/*.{js,ts,tsx,json,md}'",
    "lint": "eslint --ext .js,.ts,.tsx src",
    "build": "tsc",
    "prod": "node --env-file ./src/config/.env.local dist/config/server.js"
  },
  "author": "Florian Lepage",
  "license": "MIT",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@prisma/client": "^5.4.2",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "D": "^1.0.0",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "typescript": "^5.3.3",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/hapi__joi": "^17.1.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-prettier": "^5.1.2",
    "prettier": "^3.1.1",
    "prisma": "^5.4.2"
  }
}
